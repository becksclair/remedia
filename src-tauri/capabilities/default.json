{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main", "preview-win"],
	"permissions": [
		"core:default",
		"shell:default",
		"shell:allow-open",
		"shell:allow-execute",
		"clipboard-manager:allow-read-text",
		"dialog:default",
		"fs:default",
		"notification:default",
		"core:webview:allow-create-webview"
	]
}
